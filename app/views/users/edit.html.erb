<h1>プロフィール編集</h1>
<div class="row">
  <div class="col-md-6 offset-md-3">
    <%= form_with(model: @user) do |f| %>
      <!--<%= render 'shared/error_message' %>-->

      <%= f.label :name, "名前" %>
      <%= f.text_field :name %>
      
      <%= f.label :introduction, "自己紹介文" %>
      <%= f.text_area :introduction %>

      <%= f.submit "保存する", class: "btn btn-primary" %>
    <% end %>
    
  </div>
</div>